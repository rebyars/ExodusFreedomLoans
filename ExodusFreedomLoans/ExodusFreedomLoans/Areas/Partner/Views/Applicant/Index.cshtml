@using ExodusFreedomLoans.Utility;
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (User.IsInRole(SD.Admin) || User.IsInRole(SD.Partner))
{
    <br />
    <div class="border backgroundWhite">
        <div class="row">
            <div class="col-6">
                <h2 class="text-primary">Applicant History</h2>
            </div>
            <div class="col-6 text-right">
                <a asp-action="Upsert" class="btn btn-primary"><i class="fa-fast-backward fa-plus-square"></i> &nbsp; Submit New Applicant</a>
            </div>
        </div>

        <br />
        <br />

        <table id="tblData" class="table table-striped table-bordered" style="width:100%">
            <thread>
                <tr>
                    <th>Name</th>
                    <th>Status</th>
                    <th></th>
                </tr>

            </thread>
        </table>
    </div>
}
else
{
    <p class="text-danger">You do not have permission to access this function.</p>
}



@section Scripts{
    <script src="~/js/applicant.js"></script>
}
